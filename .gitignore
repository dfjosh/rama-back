# Rails #####################################################

# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore Byebug command history file.
.byebug_history

/config/secrets.yml
.env*

# Ignore image assets
/public/uploads

# Ember #####################################################

# compiled output
/rama-front/dist/
/rama-front/tmp/

# dependencies
/rama-front/bower_components/
/rama-front/node_modules/

# misc
/rama-front/.sass-cache
/rama-front/connect.lock
/rama-front/coverage/
/rama-front/libpeerconnection.log
/rama-front/npm-debug.log*
/rama-front/testem.log
/rama-front/yarn-error.log

# ember-try
/rama-front/.node_modules.ember-try/
/rama-front/bower.json.ember-try
/rama-front/package.json.ember-try
