<table>
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>created</th>
      <th>updated</th>
    </tr>
  </thead>
  {{#each model as |tag|}}
    <tr>
      <td>{{tag.id}}</td>
      <td>{{tag.name}}</td>
      <td>{{formatted-date tag.createdAt}}</td>
      <td>{{formatted-date tag.updatedAt}}</td>
    </tr>
  {{/each}}
</table>
