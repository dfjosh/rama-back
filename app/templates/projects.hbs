<main class="container-fluid">
  
  {{#each model as |post|}}
    {{rama-post
      post=post
      showAsterisk=false
      showFeatureImage=true
      showTaxonomy=false
      showAttribution=false
      showHorizontalRule=false
      gutterColumns=1
    }}
  {{/each}}
  
  {{rama-pagination store=store page=page}}

</main>